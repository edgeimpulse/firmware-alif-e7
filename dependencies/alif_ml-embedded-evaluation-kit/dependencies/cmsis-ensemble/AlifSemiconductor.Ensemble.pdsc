  <boards>
    <board vendor="AlifSemiconductor" name="DevKit-e7 board">
      <description>Ensemble: CPU and Base Board</description>
      <mountedDevice deviceIndex="0" Dvendor="Alif Semiconductor:165" Dname="AE722F80*"/>
	  <compatibleDevice deviceIndex="0" Dvendor="Alif Semiconductor:165" Dname="AE7*"/>
